map global goto b %{h}
map global goto n %{j}
map global goto r %{k}
map global goto s %{l}
map global goto o %{g}

map global normal <s-a> %{:w; info -markup %sh{export kak_buffile=$kak_buffile; sh ~/.config/kak/compile.sh 2>&1}<ret>}

#map global normal u %{:enter-user-mode lsp<ret>}

map global insert <tab> '<a-;>:try lsp-snippets-select-next-placeholders catch %{ execute-keys -with-hooks <lt>tab> }<ret>' -docstring 'Select next snippet placeholder'
map global lsp a %{:lsp-rename-prompt<ret>} -docstring 'lsp rename this shit'
map global lsp c %{:lsp-code-actions<ret>} -docstring 'lsp do something for me (please)'
map global normal ) %{:lsp-next-function<ret>}
map global normal ( %{:lsp-previous-function<ret>}

